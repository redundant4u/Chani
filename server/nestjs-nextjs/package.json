{
  "name": "nestnext8",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "node --inspect=0.0.0.0:9229 | nodemon src/main.ts",
    "test": "nodemon src/main.ts",
    "build:clean": "rimraf .next",
    "build:ui": "npx next build",
    "build:server": "npx tsc --project tsconfig.server.json && npx babel .next/production-server -d .next/production-server --extensions \".js\"",
    "build": "yarn build:clean && yarn build:ui && yarn build:server",
    "start": "cross-env NODE_ENV=production node .next/production-server/src/main.js",
    "make:migration": "ts-node -P tsconfig.server.json ./node_modules/typeorm/cli.js migration:generate -n",
    "migrate": "ts-node -P tsconfig.server.json ./node_modules/typeorm/cli.js migration:run",
    "migrate:rollback": "ts-node -P tsconfig.server.json ./node_modules/typeorm/cli.js migration:revert",
    "seed": "ts-node -P tsconfig.server.json ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.3",
    "@nestjs/core": "^7.6.3",
    "@nestjs/platform-express": "^7.6.3",
    "@nestjs/typeorm": "^7.1.5",
    "@types/compression": "^1.7.0",
    "@types/jquery": "^3.5.5",
    "@types/select2": "^4.0.53",
    "compression": "^1.7.4",
    "cross-env": "^6.0.3",
    "jquery": "^3.6.0",
    "nest-next": "^9.4.0",
    "next": "10",
    "pg": "^8.5.1",
    "react": "17",
    "react-dom": "17",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "select2": "^4.1.0-rc.0",
    "typeorm": "^0.2.30",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^3.5.3"
  }
}
