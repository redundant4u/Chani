{
  "name": "nestnext8",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:clean": "rimraf .next",
    "build:ui": "npx next build",
    "build:server": "npx tsc --project tsconfig.build.json && npx babel .next/production-server -d .next/production-server --extensions \".js\"",
    "build": "yarn build:clean && yarn build:ui && yarn build:server",
    "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register .next/production-server/main.js",
    "debug": "nodemon --exec 'node --inspect=0.0.0.0:9229 --require ts-node/register -r tsconfig-paths/register src/main.ts'",
    "dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "migrate": "ts-node -P tsconfig.json ./node_modules/typeorm/cli.js migration:run",
    "migrate:create": "ts-node -P tsconfig.json ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:revert": "ts-node -P tsconfig.json ./node_modules/typeorm/cli.js migration:revert",
    "seed": "ts-node -P tsconfig.json ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/typeorm": "8.0.2",
    "axios": "^0.24.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "lodash": "^4.17.21",
    "nest-next": "^9.6.0",
    "next": "^12.0.7",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@nestjs/cli": "^8.0.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "postcss": "^8.4.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "run-script-webpack-plugin": "^0.0.11",
    "tailwindcss": "^3.0.15",
    "ts-loader": "^9.2.6",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.12.0",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.3.5",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
